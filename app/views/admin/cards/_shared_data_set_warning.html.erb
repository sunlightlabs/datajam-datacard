<% if @card.has_siblings? %>
    <div class="alert alert-warning">
        <p>This card is based on a shared data set! Any changes you make to the data here will reflect in this data set's other cards:</p>
        <ul>
            <% @card.siblings.each do |card| %>
            <li><%= link_to card.title, edit_admin_card_path(card) %></li>
            <% end %>
        </ul>
    </div>
<% end rescue nil %>