<h4><%= card.title %></h4>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <% card.prepared_data(:csv)[0].each do |header| %>
        <th><%= header %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% card.prepared_data(:csv, fields: card.series)[1..-1].each do |row| %>
      <tr>
        <% row.each do |value| %>
          <td><%= value %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
